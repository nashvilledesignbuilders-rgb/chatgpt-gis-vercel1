{
  "version": 2,
  "rewrites": [
    {
      "source": "/.well-known/ai-plugin.json",
      "destination": "/.well-known/ai-plugin.json"
    }
  ],
  "headers": [
    {
      "source": "/.well-known/ai-plugin.json",
      "headers": [
        { "key": "Content-Type", "value": "application/json" },
        { "key": "Cache-Control", "value": "no-cache" }
      ]
    }
  ]
}
